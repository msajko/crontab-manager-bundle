{
    "name": "msajko/crontab-manager-bundle",
    "description": "Crontab Manager bundle for SF 3.4.x and 4.x projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ante Crnogorac",
            "email": "zwer82@gmail.com"
        }
